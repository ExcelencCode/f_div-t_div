def get_matrix(n, m, value):
    matrix = []
    for i in range(n):
        # print(i)
        matrix.append([])
        for j in range(m):
            matrix[i].append(value)
    return matrix
result1 = get_matrix(3, 4, 1)
result2 = get_matrix(2, 5, 24)
result3 = get_matrix(5, 3, 7)
print(result1)
print(result2)
print(result3)
